{
  "name": "mmd-portfolio-2022",
  "description": "The portfolio website of Matt McDonald",
  "version": "1.0.0",
  "private": true,
  "author": "Matt McDonald <mcdonamj087@gmail.com> (https://mattmcdonalddesign.com)",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "backend": "npm --prefix ../backend run dev",
    "dev": "concurrently --names 'FRONTEND,BACKEND' -c 'bgBlue.bold,bgMagenta.bold' 'gatsby develop' 'npm run backend'",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "gatsby": "4.24.8",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-postcss": "^5.24.0",
    "gatsby-plugin-sharp": "^4.25.0",
    "gatsby-source-filesystem": "^4.25.0",
    "gatsby-source-sanity": "^7.6.0",
    "gatsby-transformer-sharp": "^4.25.0",
    "gsap": "^3.11.3",
    "postcss": "^8.4.19",
    "postcss-custom-media": "^8.0.2",
    "postcss-hexrgba": "^2.1.0",
    "postcss-nested": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.40.0",
    "react-redux": "^8.0.5",
    "tsconfig-paths-webpack-plugin": "4.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "babel-preset-gatsby": "2.25.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "lint-staged": "13.0.4",
    "prettier": "2.8.0",
    "typescript": "4.9.3",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
